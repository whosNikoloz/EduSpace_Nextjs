{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/dbcontext/UserdbContext.tsx",
    "components/navbar/Notification.tsx",
    "components/Learn/Hero.tsx",
    "components/compiler/MonacoEditorWrapper.js",
    "components/compiler/ThemeDropdown.js",
    "components/compiler/Landing.js",
    "components/compiler/MonacoEditorWrapper.js",
    "components/compiler/RunIcon.jsx",
    "components/Learn/subject.jsx",
    "components/Learn/Lesson/ArrowIcon.jsx",
    "components/navbar/BeginnerIcon.jsx",
    "components/AnimatedText.jsx",
    "components/chatai/ChatAI.jsx",
    "app/not-found.tsx",
    "components/profile/UploadImg/uploadProfilePic.tsx",
    "components/profile/UploadGeneral/generalEdit.tsx",
    "components/profile/userCardSkeleton.jsx"
, "components/profile/UploadImg/cropimg.tsx", "components/profile/ChangeEmail/confirmAccess.tsx"  ],
  "exclude": ["node_modules"]
}
